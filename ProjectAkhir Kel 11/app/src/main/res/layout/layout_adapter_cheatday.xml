<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/nama_cheatday"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="8dp"
        android:text="Nama: "
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tanggal_cheatday"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="8dp"
        android:text="Tanggal: "
        android:textSize="12dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nama_cheatday" />

    <TextView
        android:id="@+id/bulan_cheatday"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="8dp"
        android:text="Bulan: "
        android:textSize="12dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tanggal_cheatday" />

    <TextView
        android:id="@+id/tahun_cheatday"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="8dp"
        android:paddingBottom="8dp"
        android:text="Tahun: "
        android:textSize="12dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bulan_cheatday"/>

    <TextView
        android:id="@+id/tahun_cheatday2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="8dp"
        android:text="a"
        android:textSize="12dp"
        app:layout_constraintStart_toEndOf="@+id/tahun_cheatday"
        app:layout_constraintTop_toBottomOf="@+id/bulan_cheatday" />

    <TextView
        android:id="@+id/bulan_cheatday2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="8dp"
        android:layout_marginStart="11dp"
        android:text="a"
        android:textSize="12dp"
        app:layout_constraintStart_toEndOf="@+id/bulan_cheatday"
        app:layout_constraintTop_toBottomOf="@+id/tanggal_cheatday" />

    <TextView
        android:id="@+id/tanggal_cheatday2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="8dp"
        android:layout_marginStart="27dp"
        android:text="a"
        android:textSize="12dp"
        app:layout_constraintStart_toEndOf="@+id/tanggal_cheatday"
        app:layout_constraintTop_toBottomOf="@+id/nama_cheatday" />

    <TextView
        android:id="@+id/nama_cheatday2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="8dp"
        android:text="a"
        android:textSize="20dp"
        app:layout_constraintStart_toEndOf="@+id/nama_cheatday"
        app:layout_constraintTop_toTopOf="parent" />



    <TextView
        android:id="@+id/textView32"
        android:layout_width="match_parent"
        android:layout_height="4dp"
        android:background="@color/black"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>